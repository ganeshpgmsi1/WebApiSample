using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using UniversalWeather.CMS.API.Domain.Catalog;
using System.Data.Entity.ModelConfiguration;

namespace UniversalWeather.CMS.API.Data.Mapping
{
    /// <summary>
    /// Custom mapping configuration
    /// </summary>
    public class ManufacturerTemplateConfiguration : EntityTypeConfiguration<ManufacturerTemplate>
    {
        /// <summary>
        /// Set mapping values on initialization
        /// </summary>
        public ManufacturerTemplateConfiguration()
        {
            ToTable("ManufacturerTemplate");
            HasKey(p => p.ManufacturerTemplateId);
        }
    }
}
